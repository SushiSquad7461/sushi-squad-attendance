{
  "name": "sushi-squad-attendance-pages",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "tailwind": "npx tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "serve": "vite",
    "dev": "concurrently \"yarn tailwind\" \"yarn serve\"",
    "build": "npx tailwindcss -i ./src/styles.css -o ./dist/styles.css && tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "firebase": "^10.7.1"
  },
  "packageManager": "yarn@4.0.2"
}
